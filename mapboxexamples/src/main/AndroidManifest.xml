<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.mapbox.navigation.examples.core" >
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxNavigationActivity"
            android:label="@string/title_navigation">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxRouteLineAndArrowActivity"
            android:label="@string/title_route_line"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.camera.MapboxCameraAnimationsActivity"
            android:label="@string/title_camera"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxSnapshotActivity"
            android:label="@string/title_snapshotter"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxSignboardActivity"
            android:label="@string/title_signboard"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxJunctionActivity"
            android:label="@string/title_junction"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxTripProgressActivity"
            android:label="@string/title_trip_progress">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxManeuverActivity"
            android:label="@string/title_maneuver">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxVoiceActivity"
            android:label="@string/title_voice">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.replay.HistoryFilesActivity"
            android:label="@string/title_replay" >
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.ReplayHistoryActivity"
            android:label="@string/title_replay" >
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxCustomStyleActivity"
            android:label="@string/title_custom_style" >
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxMultipleArrowActivity"
            android:label="@string/title_multiple_arrows" >
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.util.RouteDrawingActivity"
            android:label="@string/title_draw_utility" >
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.IndependentRouteGenerationActivity"
            android:label="@string/title_independent_route">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.core.MapboxBuildingHighlightActivity"
            android:label="@string/title_trip_progress">
        </activity>

        <activity
            android:name="com.wonder.mapbox.mapboxexamples.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>

    </application>

</manifest>